#!/usr/bin/env bash

#
# docker-volume-netshare properties
#

# Directory to store the DOCKER_VOLUME_NETSHARE logs
export DOCKER_VOLUME_NETSHARE_LOG_DIR=$LOG_DIR

# Directory to store the DOCKER_VOLUME_NETSHARE process IDs
export DOCKER_VOLUME_NETSHARE_PID_DIR=$RUN_DIR

# Directory to store the DOCKER_VOLUME_NETSHARE data files
export DOCKER_VOLUME_NETSHARE_STORE_DIR=$STORE_DIR

# Directory to store the DOCKER_VOLUME_NETSHAREtemp files
export DOCKER_VOLUME_NETSHARE_TMP_DIR=$TMP_DIR
